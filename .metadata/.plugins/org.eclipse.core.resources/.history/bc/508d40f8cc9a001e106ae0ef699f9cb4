package module_3;

interface ATM { 
void deposit(); 
void withdrawl(); 
} 
class Cust implements ATM 
{ 
@Override
public void deposit() { 
 System.out.println("Please deposit ur amount"); 
 } 
@Override
public void withdrawl() { 
 System.out.println("Collect ur amount"); 
 } 
}

    public class User {
    	
      public static void main(String[] args) {
	     Cust s1=new Cust(); 
		 s1.deposit(); 
		 s1.withdrawl(); 
		 } 
		
}


